{
  "name": "gignore-cli",
  "version": "1.0.5",
  "description": "A simple CLI tool for generating .gitignore file",
  "author": "konekoya",
  "repository": {
    "type": "git",
    "url": "https://github.com/konekoya/gignore-cli.git"
  },
  "homepage": "https://github.com/konekoya/gignore-cli",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn typecheck"
    }
  },
  "bin": "/dist/index.js",
  "dependencies": {
    "@types/node": "^14.14.7",
    "@types/ora": "^3.2.0",
    "@types/prompts": "^2.0.9",
    "@types/yargs": "^15.0.9",
    "axios": "^0.21.0",
    "chalk": "^4.1.0",
    "husky": "^4.3.0",
    "ora": "^5.1.0",
    "prompts": "^2.4.0",
    "typescript": "^4.0.5",
    "yargs": "^16.1.0"
  },
  "keywords": [
    ".gitignore",
    "gitignore"
  ]
}
